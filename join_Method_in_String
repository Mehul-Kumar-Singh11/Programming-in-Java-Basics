
public class join_Method {

	public static void main(String[] args) {
		String s1 = "MEHUL";
		String s2 = "Kumar";
		String s3 = "Singh";
		
		System.out.println(String.join(";",s1,s2));
		System.out.println(String.join("a",null,null));
		System.out.println(String.join("a","hello",null));
//		System.out.println(String.join(null,null,null)); // error
		

	}

}
